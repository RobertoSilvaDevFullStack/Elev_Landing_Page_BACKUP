# Configuração de Ambiente - Exemplo
# Copie para .env.local e preencha com os valores reais

# Facebook Pixel ID (obrigatório)
NEXT_PUBLIC_FACEBOOK_PIXEL_ID=your_facebook_pixel_id_here

# RD Station Token (obrigatório)
NEXT_PUBLIC_RDSTATION_TOKEN=your_rdstation_token_here

# WhatsApp Business Number (formato: 5511999999999)
NEXT_PUBLIC_WHATSAPP_NUMBER=55119999999999

# Environment
NODE_ENV=development

# MySQL Database Configuration (para APIs PHP)
DB_HOST=localhost
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_PORT=3306